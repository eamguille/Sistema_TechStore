@model Techstore_WebApp.Models.DB.Usuario

@{
    ViewData["Title"] = "Perfil de Usuario";
}

<h2 class="mb-5" >Perfil de Usuario</h2>
<div class="card">
    <div class="card-header">
        Información de mi perfil
    </div>
    <div class="card-body">
        <p><strong>Nombre de Usuario:</strong> @Model.NombreUsuario</p>
        <p><strong>Nombres:</strong> @Model.Nombres</p>
        <p><strong>Apellidos:</strong> @Model.Apellidos</p>
        <p><strong>Email:</strong> @Model.Email</p>
        <p><strong>Teléfono:</strong> @Model.Telefono</p>
        <p><strong>Dirección:</strong> @Model.Direccion</p>
        <p><strong>Rol:</strong> @Model.Rol</p>
    </div>
</div>

